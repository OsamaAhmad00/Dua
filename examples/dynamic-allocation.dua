no_mangle int printf(str message, ...);

void print(int n) { printf("%d", n); }


// Case Object allocation
// Outputs "Hello123Bye"

class X
{
    int num;

    constructor(int i)
    {
        printf("Hello");
        print(i);
    }

    destructor
    {
        printf("Bye");
    }
}

int main()
{
    X* x = new X(1);
    x->num = 23;
    print(x->num);
    delete x;
}